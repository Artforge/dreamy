source 'https://rubygems.org'

gem 'rails', '3.2.2'

gem 'pg'

# Gems used only for assets and not required
# in production environments by default.
group :assets do
  gem 'sass-rails',   '~> 3.2.3'
  gem 'coffee-rails', '~> 3.2.1'
  gem 'uglifier',     '>= 1.0.3'
  gem 'zurb-foundation'
  gem 'jquery-rails'
  gem 'rails-backbone'
  gem 'eco'
end

gem 'rspec-rails', groups: [:development, :test]
